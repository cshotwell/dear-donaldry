{
  "name": "Dear Donald",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Appending Dear Diary to @realDonaldTrump twitters",
  "homepage_url": "twitter.com/carl_talks",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "permissions": [
    "http://twitter.com/realDonaldTrump/*",
    "https://twitter.com/realDonaldTrump/*",
    ""
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
